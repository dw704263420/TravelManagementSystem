<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--全局3、将configuration更改成mapper sql方法和语句均写在一个命名空间的mapper下-->
<!--复合查询，因此用映射来处理下-->
<mapper namespace="com.kuang.dao.MemberMapper">
<select id="findMemberById" resultType="Member">
    select * from  ssm.member where id = #{memberId]}
</select>
</mapper>